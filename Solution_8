/*
**Question 8**

Given the array nums consisting of 2n elements in the form [x1,x2,...,xn,y1,y2,...,yn].

*Return the array in the form* [x1,y1,x2,y2,...,xn,yn].  */


vector<int> arranging(vector<int>v , int m ){
    int n = v.size(); 
    vector<int> ans; 
    int i = 0; 
    while(m<n){
       ans.push_back(v[i++]);
       ans.push_back(v[m++]);
   }
   return ans; 
}
